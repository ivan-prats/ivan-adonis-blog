{
  "name": "ivan-adonis-blog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "test:raw": "node -r @adonisjs/assembler/build/register japaFile.ts",
    "test:unit": "TESTING_TYPE=unit npm run test:raw --silent -- --no-db",
    "test:integration": "TESTING_TYPE=integration npm run test:raw --silent",
    "test:system": "TESTING_TYPE=system npm run test:raw --silent",
    "test:all": "npm run test:unit --silent && npm run test:integration --silent && npm run test:system --silent && npm run build && npm run prepare-deploy"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.4.1",
    "@symfony/webpack-encore": "^1.7.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/markdown-it": "^12.2.3",
    "@types/sinon": "^10.0.6",
    "@types/supertest": "^2.0.11",
    "adonis-preset-ts": "^2.1.0",
    "alpinejs": "^3.8.1",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "japa": "^4.0.0",
    "jsdom": "^19.0.0",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.3.0",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "sinon": "^12.0.1",
    "supertest": "^6.2.1",
    "tailwindcss": "^3.0.15",
    "typescript": "~4.5",
    "youch": "^3.1.0",
    "youch-terminal": "^2.0.1"
  },
  "dependencies": {
    "@adonisjs/core": "^5.4.2",
    "@adonisjs/repl": "^3.1.8",
    "@adonisjs/session": "^6.1.2",
    "@adonisjs/shield": "^7.0.7",
    "@adonisjs/view": "^6.1.1",
    "markdown-it": "^12.3.2",
    "markdown-it-meta": "^0.0.1",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "slugify": "^1.6.5",
    "source-map-support": "^0.5.21"
  }
}
